{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
	  "scripts": ["background.js"]
  },

  "commands":{
	  "_execute_browser_action":{
		  "suggested_key": {
			  "default": "Alt+L"
		  }
	  }
  }
}
